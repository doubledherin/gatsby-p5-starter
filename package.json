{
  "name": "gatsby-p5-starter",
  "version": "0.0.1",
  "description": "Gatsby starter website template for use with p5.js",
  "author": "Wendy Dherin",
  "license": "MIT",
  "private": true,
  "keywords": [
    "gatsby",
    "graphQL",
    "p5js",
    "react",
    "example",
    "p5.js",
    "starter",
    "react-typography",
    "typography",
    "gallery",
    "css-grid"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/doubledherin/gatsby-p5-starter.git"
  },
  "bugs": {
    "url": "https://github.com/doubledherin/gatsby-p5-starter/issues"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "gatsby clean",
    "reset": "rm -rf .cache/ public/ node_modules/ package-lock.json",
    "prep": "rm -rf .cache/ public/ node_modules/ && npm ci",
    "develop": "DEBUG=gatsby* gatsby develop --verbose",
    "build": "DEBUG=gatsby* gatsby build --verbose",
    "serve": "DEBUG=gatsby* gatsby serve --verbose",
    "test": "echo \"Error: no test specified -- https://gatsby.app/unit-testing/\" && exit 1",
    "deploy-gh": "gatsby clean && gatsby build && gh-pages -d public"
  },
  "dependencies": {
    "gatsby": "^2.24.11",
    "gatsby-image": "^2.4.0",
    "gatsby-plugin-sass": "^2.3.1",
    "gatsby-plugin-sharp": "^2.6.0",
    "gatsby-plugin-typography": "^2.5.0",
    "gatsby-source-filesystem": "^2.3.0",
    "gatsby-transformer-sharp": "^2.5.0",
    "node-sass": "^7.0.0",
    "p5": "^0.9.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.0",
    "react-dom": "^16.10.0",
    "react-helmet": "^6.0.0",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-bootstrap": "^0.16.19"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.20.5",
    "gh-pages": "^2.2.0"
  }
}
